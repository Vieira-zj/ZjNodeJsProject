    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-enough": "0.3.0",
    "eslint-loader": "2.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "koa-connect": "2.0.1",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
